<!-- sidebar -->
<div class="sidebar">
  <%= render 'shared/user', user: @user %>
  <%= render 'shared/new', book: @book %>
</div>


<% @users.each do |user| %>
  <div>
    
    <p><%= image_tag user.get_profile_image(100,100)  %></p>

    <p><%= user.name %></p>
    <p><%= link_to 'show', user_path(user) %></p>
  </div>
<% end %>